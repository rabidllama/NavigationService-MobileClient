<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="de.uniheidelberg.geog.navigationmobileclient.NavigationClientActivity" 
    android:orientation="vertical">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/myaccessibleeu_logo_text"
        android:contentDescription="@string/app_name" />
    
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_margin="10dp">
        
        <LinearLayout 
            android:baselineAligned="false"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            
            <de.uniheidelberg.geog.navigationmobileclient.SquareLayout
       			android:layout_width="wrap_content"
       			android:layout_height="wrap_content"
       			android:layout_weight="1">
       			
                <Button android:id="@+id/main_continue_button"
           			android:layout_width="wrap_content"
           			android:layout_height="match_parent"
           			android:background="@drawable/return_button" />
                
            </de.uniheidelberg.geog.navigationmobileclient.SquareLayout>
            
             <de.uniheidelberg.geog.navigationmobileclient.SquareLayout
       			android:layout_width="wrap_content"
       			android:layout_height="wrap_content"
       			android:layout_weight="1">
       			
                <Button android:id="@+id/main_route_button"
           			android:layout_width="wrap_content"
           			android:layout_height="match_parent"
           			android:background="@drawable/route_button" />
                
            </de.uniheidelberg.geog.navigationmobileclient.SquareLayout>
            
        </LinearLayout>
        
        <LinearLayout 
            android:baselineAligned="false"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            
            <de.uniheidelberg.geog.navigationmobileclient.SquareLayout
       			android:layout_width="wrap_content"
       			android:layout_height="wrap_content"
       			android:layout_weight="1">
       			
                <Button android:id="@+id/main_poi_button"
           			android:layout_width="wrap_content"
           			android:layout_height="match_parent"
           			android:background="@drawable/poi_button" />
                
            </de.uniheidelberg.geog.navigationmobileclient.SquareLayout>
            
             <de.uniheidelberg.geog.navigationmobileclient.SquareLayout
       			android:layout_width="wrap_content"
       			android:layout_height="wrap_content"
       			android:layout_weight="1">
       			
                <Button android:id="@+id/main_settings_button"
           			android:layout_width="wrap_content"
           			android:layout_height="match_parent"
           			android:background="@drawable/settings_button" />
                
            </de.uniheidelberg.geog.navigationmobileclient.SquareLayout>
            
        </LinearLayout>
        
    </LinearLayout>
     
